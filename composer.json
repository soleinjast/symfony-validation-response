{
  "name": "soleinjast/symfony-validation-response",
  "description": "Clean and customizable validation error responses for Symfony APIs",
  "type": "symfony-bundle",
  "keywords": ["symfony", "validation", "api", "dto", "error-handling", "json", "rest"],
  "license": "MIT",
  "authors": [
    {
      "name": "Soleinjast",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.3|^7.0",
    "symfony/http-kernel": "^6.3|^7.0",
    "symfony/validator": "^6.3|^7.0",
    "symfony/dependency-injection": "^6.3|^7.0",
    "symfony/config": "^6.3|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.3|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Soleinjast\\ValidationResponse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soleinjast\\ValidationResponse\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}